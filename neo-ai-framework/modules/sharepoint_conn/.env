# Credenciais da AWS
AWS_ACCESS_KEY_ID=sua_access_key_id
AWS_SECRET_ACCESS_KEY=sua_secret_access_key
AWS_REGION=us-east-1

# Configuração do bucket S3
S3_BUCKET_NAME=nome_do_seu_bucket
S3_PREFIX=pasta/opcional/

# Credenciais do Microsoft Azure/SharePoint
TENANT_ID=seu_tenant_id
CLIENT_ID=seu_client_id
CLIENT_SECRET=seu_client_secret

# Configuração do SharePoint
SITE_ID=id_do_site_sharepoint
DRIVE_ID=id_da_biblioteca_documentos
SHAREPOINT_FOLDER=/Shared Documents/S3Data